{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.js", "*.css", "*.ico", "*.png", "*.svg", "*.woff2"]
  },
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/index.html"
    },
    {
      "route": "/infinity",
      "rewrite": "/index.html"
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}
